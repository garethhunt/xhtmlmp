<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:aboutURL>chrome://xhtmlmp/content/about/about.xul</em:aboutURL>
        <em:creator>Gareth Hunt</em:creator>
        <em:description>Allows XHTML Mobile Profile to be displayed in the browser.</em:description>
        <em:homepageURL>@build.homepage@</em:homepageURL>
        <em:iconURL></em:iconURL>
        <em:id>{8ea9957e-2953-402f-80e0-bceb5f169d6f}</em:id>
        <em:name>XHTML Mobile Profile</em:name>
        <em:optionsURL>chrome://xhtmlmp/content/about/about.xul</em:optionsURL>
        <em:version>@build.version@</em:version>
        <em:file>
            <Description about="urn:mozilla:extension:file:xhtmlmp.jar">
                <em:locale>locale/en-US/</em:locale>
                <em:package>content/</em:package>
            </Description>
        </em:file>
        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>3.5</em:minVersion>
                <em:maxVersion>4.0.*</em:maxVersion>
            </Description>
        </em:targetApplication>
        <!-- Seamonkey 2.0 -->
        <em:targetApplication>
          <Description>
            <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
            <em:minVersion>2.0</em:minVersion>
            <em:maxVersion>2.0.*</em:maxVersion>
          </Description>
        </em:targetApplication>
    </Description>
</RDF>
